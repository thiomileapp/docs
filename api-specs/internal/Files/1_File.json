{
    "paths": {
        "/file/view": {
            "get": {
                "summary": "View file",
                "tags": [
                    "File"
                ],
                "operationId": "readFile",
                "deprecated": false,
                "produces": [
                    "image/png",
                    "image/jpg",
                    "image/jpeg"
                ],
                "parameters": [
                    {
                        "name": "url",
                        "in": "query",
                        "required": true,
                        "type": "string",
                        "description": "File url that generated by system.<br> **Example**: `https://apistorage.mile.app/sandbox/6299add8/2024/03/20/1710920997.jpg`"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "schema": {
                            "type" : "file"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                          "$ref": "#/parameters/responses/badRequest/schema"
                        }
                    },
                    "500": {
                      "description": "Server error",
                      "schema": {
                        "$ref": "#/parameters/responses/serverError/schema"
                      }
                    }
                }
            }
        }
    }
}