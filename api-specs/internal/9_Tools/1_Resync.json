    "definitions": {
        "resync-data": {
            "example": {
                "id" : ["64c208fdec80975f0677b484"],
                "type" : "task",
                "skipAutomation" : false,
                "events" : ["onTaskCreated"]
            },
            "type": "object",
            "properties": {
                "id": {
                    "type": "array",
                    "maxItems" : 200,
                    "description": "Unique identifier for the data that generated by system. **Example:** `['6352736c628401059b37a2e2','6352736c628401059b37a2e2']`"
                },
                "type": {
                    "type": "string",
                    "description": "Type of data that want to resync. **Example:** `routing`",
                    "enum" : ["routing,task,dataSource,automation"]
                },
                "skipAutomation": {
                    "type": "boolean",
                    "description": "If the user wants to trigger automation, the user must fill in the `events` and set `skipAutomation` to false. **Default:** `true`"
                },
                "events": {
                    "type": "array",
                    "description": "Define automations events that you want to execute. The user can fill in `events` according to predefined `type`, as follows : <br> **routing** : `onRoutingResultFinished`, `onRoutingResultDispatched` <br> **task** : `onTaskCreated`, `onTaskFinished`, `onTaskAssigned` <br> **dataSource** : `onDataSourceCreated`.",
                    "enum" : ["onTaskCreated,onTaskFinished,onTaskAssigned,onDataSourceCreated,onRoutingResultFinished"]
                }
            },
            "required": [
                "id",
                "type"
            ]
        },
        "resync-data-v2": {
            "example": {
                "type" : "organization",
                "dateFrom" : "2024-06-17 00:00:00",
                "dateTo" : "2024-07-18 23:59:59"
            },
            "type": "object",
            "properties": {
                "type": {
                    "type": "string",
                    "description": "Type of data that want to resync. **Example:** `routing`",
                    "enum" : ["organization,routing,user,datasource,automation"]
                },
                "dateFrom": {
                    "type": "string",
                    "description": "Start filter for date created from"
                },
                "dateTo": {
                    "type": "string",
                    "description": "End filter for date created from"
                }
            },
            "required": [
                "id",
                "type"
            ]
        },
