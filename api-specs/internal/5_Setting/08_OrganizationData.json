                        "$ref": "#/parameters/pageInQuery"
                    },
                    {
                        "$ref": "#/parameters/limitInQuery"
                    },
                    {
                        "name": "name",
                        "in": "query",
                        "required": false,
                        "type": "string",
                        "description": "Exact match filter for organization data name (case-sensitive). This field will filter organization data using exact name matching. **Example:** `default_config` will only match records with exactly `default_config` as the name."
                    },
                    {
                        "name": "q",
                        "in": "query",
                        "required": false,
                        "type": "string",
                        "description": "Case-insensitive partial match search for organization data names. This field will search organization data using partial matching (substring search) that is case-insensitive. **Example:** `config` will match `CONFIG`, `MyConfig`, `test_config_v2`, etc."
                    },
                    {
                        "name": "sortBy",
                        "in": "query",
                        "required": false,
                        "type": "string",
                        "description": "sortBy is used to sort the result-set in ascending or descending order according to selected field. The value of this param is one of available fields on organization data objects.<br>**Example:** `name`, `createdTime`, etc. **Default:** `name`"
                    },
                    {
                        "name": "sortOrder",
                        "in": "query",
                        "required": false,
                        "type": "string",
                        "description": "sortOrder is to decide ascending or descending order **Example:** `asc` or `desc`. **Default:** `asc`"
                    },
                    {
                        "name": "fields",
                        "in": "query",
                        "required": false,
                        "type": "string",
                        "description": "Fields is for selecting the specific fields that you can choose by multiples to show in the response using a string with a comma delimiter.<br>**Example:** `name,data,createdTime`"
                    }
