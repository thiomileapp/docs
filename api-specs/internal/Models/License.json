          },
          "allowExtraDataSourceType": {
            "type": "boolean",
            "description": "Flag to indicate if extra data source types are allowed."
          },
          "sla": {
            "type": "boolean",
            "description": "Flag to indicate if SLA is included."
          },
          "sso": {
            "type": "boolean",
